a:7:{i:83;a:42:{s:9:"seo_title";s:34:"Canon EF 100mm f/2.8L Macro IS USM";s:6:"seo_h1";s:34:"Canon EF 100mm f/2.8L Macro IS USM";s:10:"product_id";s:2:"83";s:4:"name";s:34:"Canon EF 100mm f/2.8L Macro IS USM";s:11:"description";s:5030:"&lt;p&gt;
